{
  "name": "@<%= partition %>/<%= group %>-provisioning",
  "version": "<%= version %>",
  "description": "",
  "main": "stacks/infrastructure.js",
  "scripts": {
   
    "nuke": "rm -rf ./node_modules ./package-lock.json ./cdk.out",     
    "clean": "rm -rf ./dist ./out  ./coverage",
    "build": "tsc && cdk synth",
    "watch": "tsc -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all",
    "destroy": "cdk destroy --all"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "18.11.18",
    "aws-cdk": "^2.61.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "@tuxounet-k2/builder": "latest",
    <% for(const component of components) { %>
    "@<%= partition %>/<%= group %>-<%= component %>-infra" : "<%= version %>",
    <% } %>
    "aws-cdk-lib": "^2.61.1",
    "constructs": "^10.0.0"
  }
}